      {
        "id": "node-company-understanding",
        "type": "custom",
        "position": {
          "x": -100,
          "y": 100
        },
        "data": {
          "label": "1. Company Understanding",
          "type": "LLM",
          "status": "success",
          "inputs": [
            "raw_lead"
          ],
          "outputs": [
            "company_understanding"
          ],
          "systemPrompt": "# ROLE: AOS Module 1 (Operational Analysis)\n# GOAL: Analyze the provided unstructured company data to extract key business information.\n# INPUT\n{company_input}\n# OUTPUT SCHEMA (JSON):\n{\n  \"company_summary\": \"Brief description of what the company does\",\n  \"what_they_sell\": \"Products or services offered\",\n  \"business_model\": \"How they create value\",\n  \"revenue_model\": \"How they get paid\",\n  \"icp\": {\n    \"buyer_titles\": [\"CEO\", \"Operations Director\"],\n    \"buyer_org_type\": \"Type of organizations\",\n    \"geo\": [\"Locations\"],\n    \"industry\": [\"Industries\"]\n  },\n  \"positioning_claims\": [\"Key claims they make\"],\n  \"confidence\": 1-10\n}",
          "userPrompt": "Analyze this lead:\n{{node-box1-input.output}}",
          "model": "gemini-3-pro-preview",
          "temperature": 0.4,
          "schema": {
            "type": "object",
            "properties": {
              "company_summary": { "type": "string" },
              "what_they_sell": { "type": "string" },
              "business_model": { "type": "string" },
              "revenue_model": { "type": "string" },
              "icp": {
                "type": "object",
                "properties": {
                  "buyer_titles": { "type": "array", "items": { "type": "string" } },
                  "buyer_org_type": { "type": "string" },
                  "geo": { "type": "array", "items": { "type": "string" } },
                  "industry": { "type": "array", "items": { "type": "string" } }
                }
              },
              "positioning_claims": { "type": "array", "items": { "type": "string" } },
              "confidence": { "type": "integer", "minimum": 0, "maximum": 10 }
            },
            "required": ["company_summary", "what_they_sell", "business_model", "revenue_model", "icp", "positioning_claims", "confidence"]
          }
        },
        "width": 200,
        "height": 107,
        "selected": true,
        "dragging": false
      },
      {
