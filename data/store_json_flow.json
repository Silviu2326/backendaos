          "type": "LLM",
          "status": "success",
          "inputs": [
            "raw_lead",
            "company_understanding"
          ],
          "outputs": [
            "persona_understanding"
          ],
          "systemPrompt": "# ROLE: Persona Analysis\n# GOAL: Generate persona understanding from lead and company data.\n# OUTPUT SCHEMA (JSON):\n{\n  \"persona_label\": \"Label for this persona\",\n  \"archetype\": \"Personality archetype\",\n  \"motivations\": [\"Key motivations\"],\n  \"kpis\": [\"Key performance indicators they care about\"],\n  \"priorities\": [\"Business priorities\"],\n  \"do\": [\"What they do\"],\n  \"dont\": [\"What they avoid\"],\n  \"confidence\": 1-10\n}",
          "userPrompt": "Generate persona for:\nLead: {{node-box1-input.output}}\nCompany: {{node-company-understanding}}",
          "model": "gemini-3-pro-preview",
          "temperature": 0.5,
          "schema": {
            "type": "object",
            "properties": {
              "persona_label": { "type": "string" },
              "archetype": { "type": "string" },
              "motivations": { "type": "array", "items": { "type": "string" } },
              "kpis": { "type": "array", "items": { "type": "string" } },
              "priorities": { "type": "array", "items": { "type": "string" } },
              "do": { "type": "array", "items": { "type": "string" } },
              "dont": { "type": "array", "items": { "type": "string" } },
              "confidence": { "type": "integer", "minimum": 0, "maximum": 10 }
            },
            "required": ["persona_label", "archetype", "motivations", "kpis", "priorities", "do", "dont", "confidence"]
          }
        },
        "width": 200,
        "height": 135,
        "selected": false,
        "dragging": false
