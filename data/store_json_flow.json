      {
        "id": "node-basecontribution",
        "type": "custom",
        "position": {
          "x": 500,
          "y": 100
        },
        "data": {
          "label": "3. Feasibility + Base Contribution + Base Angle",
          "type": "LLM",
          "status": "success",
          "inputs": [
            "company_understanding",
            "persona_understanding",
            "raw_lead"
          ],
          "outputs": [
            "base_contribution"
          ],
          "systemPrompt": "# ROLE: Base Contribution Analysis\n# GOAL: Evaluate feasibility and generate base angle for outreach.\n# OUTPUT SCHEMA (JSON):\n{\n  \"cao_with_aos\": {\n    \"one_liner\": \"Brief description\",\n    \"expanded\": [\"Expanded points\"],\n    \"icp_targeting_statement\": \"Target market statement\"\n  },\n  \"base_angle\": {\n    \"one_liner\": \"Hook angle\",\n    \"intensity_score\": 1-10\n  },\n  \"feasibility\": {\n    \"is_feasible\": true/false,\n    \"reasons\": [\"Reasons for feasibility\"],\n    \"risks\": [\"Potential risks\"]\n  }\n}",
          "userPrompt": "Generate base contribution for:\nCompany: {{node-company-understanding}}\nPersona: {{node-persona-understanding}}\nLead: {{node-box1-input.output}}",
          "model": "gemini-3-pro-preview",
          "temperature": 0.35,
          "schema": {
            "type": "object",
            "properties": {
              "cao_with_aos": {
                "type": "object",
                "properties": {
                  "one_liner": { "type": "string" },
                  "expanded": { "type": "array", "items": { "type": "string" } },
                  "icp_targeting_statement": { "type": "string" }
                }
              },
              "base_angle": {
                "type": "object",
                "properties": {
                  "one_liner": { "type": "string" },
                  "intensity_score": { "type": "integer", "minimum": 0, "maximum": 10 }
                }
              },
              "feasibility": {
                "type": "object",
                "properties": {
                  "is_feasible": { "type": "boolean" },
                  "reasons": { "type": "array", "items": { "type": "string" } },
                  "risks": { "type": "array", "items": { "type": "string" } }
                }
              }
            },
            "required": ["cao_with_aos", "base_angle", "feasibility"]
          }
        },
        "width": 200,
        "height": 150,
        "selected": false,
        "dragging": false
