          "systemPrompt": "# ROLE: Box1 Analysis\n# GOAL: Determine if lead is FIT, HIT, or DROP based on analysis.\n# CRITICAL: This determines if lead proceeds in pipeline.\n# OUTPUT SCHEMA (JSON):\n{\n  \"status\": \"FIT or HIT or DROP\",\n  \"confidence\": 1-10,\n  \"reasoning\": \"Why this classification\",\n  \"instantly_body\": {\n    \"body1\": \"Opening line for email\",\n    \"body2\": \"Value proposition\",\n    \"body3\": \"Call to action\",\n    \"body4\": \"Signature/close\"\n  },\n  \"storage_notes\": \"Notes for storage if FIT but not HIT\"\n}",
          "userPrompt": "Analyze for Box1 decision:\nCompany: {{node-company-understanding}}\nPersona: {{node-persona-understanding}}\nBase: {{node-basecontribution}}",
          "model": "gemini-3-pro-preview",
          "temperature": 0.3,
          "schema": {
            "type": "object",
            "properties": {
              "status": { "type": "string", "enum": ["FIT", "HIT", "DROP"] },
              "confidence": { "type": "integer", "minimum": 0, "maximum": 10 },
              "reasoning": { "type": "string" },
              "instantly_body": {
                "type": "object",
                "properties": {
                  "body1": { "type": "string" },
                  "body2": { "type": "string" },
                  "body3": { "type": "string" },
                  "body4": { "type": "string" }
                }
              },
              "storage_notes": { "type": "string" }
            },
            "required": ["status", "confidence", "reasoning", "instantly_body"]
          }
        },
        "width": 200,
        "height": 120,
        "selected": false,
        "dragging": false
      },
